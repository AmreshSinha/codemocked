{
  "name": "codemocked",
  "version": "0.0.0",
  "private": true,
  "description": "Self-hostable open-source IDE",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=8.11.0",
    "node": ">=16.14.2"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.15",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmreshSinha/codemocked.git"
  },
  "author": "AmreshSinha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AmreshSinha/codemocked/issues"
  },
  "homepage": "https://github.com/AmreshSinha/codemocked#readme",
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,jsx,md}": "prettier --write"
  }
}
